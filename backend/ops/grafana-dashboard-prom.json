{
  "title": "Guardrail Overview",
  "panels": [
    {
      "type": "timeseries",
      "title": "p95 Evaluate Latency (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(guardrail_eval_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "s"}}
    },
    {
      "type": "timeseries",
      "title": "Requests by Decision (5m rate)",
      "targets": [
        {
          "expr": "sum(rate(guardrail_requests_total[5m])) by (decision)",
          "legendFormat": "{{decision}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Failures by Reason (5m rate)",
      "targets": [
        {
          "expr": "sum(rate(guardrail_failures_total[5m])) by (reason)",
          "legendFormat": "{{reason}}"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "version": 1
}

